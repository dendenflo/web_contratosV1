<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Web Austral Servicios</title>

    <environment include="Development">
        <!-- #region PLANTILLA -->
        <link rel="stylesheet" href="~/vendors/iconfonts/font-awesome/css/all.min.css">
        <link rel="stylesheet" href="~/vendors/css/vendor.bundle.base.css">
        <link rel="stylesheet" href="~/vendors/css/vendor.bundle.addons.css">
        <link rel="stylesheet" href="../../vendors/summernote/dist/summernote-bs4.css">
        <link asp-append-version="true" rel="stylesheet" href="~/css/style.css">
        <link href="~/vendors/toastr/toastr.min.css" rel="stylesheet" />
        <!-- #endregion -->
    </environment>
    <environment exclude="Development">
        <!-- #region PLANTILLA -->
        <link rel="stylesheet" href="~/vendors/iconfonts/font-awesome/css/all.min.css">
        <link rel="stylesheet" href="~/vendors/css/vendor.bundle.base.css">
        <link rel="stylesheet" href="~/vendors/css/vendor.bundle.addons.css">
        <link rel="stylesheet" href="../../vendors/summernote/dist/summernote-bs4.css">
        <link asp-append-version="true" rel="stylesheet" href="~/css/style.css">
        <link href="~/vendors/toastr/toastr.min.css" rel="stylesheet" />
        <!-- #endregion -->
    </environment>
</head>
<body class="sidebar-dark">
    <div class="container-scroller">
        <!-- #region BARRA DE NAVEGACIÓN CABECERA -->
        <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row default-layout-navbar">
            <div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-center">
                <a class="navbar-brand brand-logo-mini" href="#"><img src="~/images/logo_mini.png" alt="logo" /></a>
            </div>
            <div class="navbar-menu-wrapper d-flex align-items-stretch">
                <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
                    <span class="fas fa-bars"></span>
                </button>
                <ul class="navbar-nav navbar-nav-right">
                    <li class="nav-item nav-profile dropdown">
                        <a class="nav-link dropdown-toggle" href="#" data-toggle="dropdown" id="profileDropdown">
                            <img src="~/images/faces/user.png" alt="profile" />
                        </a>
                        <div class="dropdown-menu dropdown-menu-right navbar-dropdown" aria-labelledby="profileDropdown">
                            <div class="dropdown-divider"></div>
                            <a href="~/account/logout" class="dropdown-item">
                                <i class="fas fa-power-off text-primary"></i>
                                Salir
                            </a>
                        </div>
                    </li>
                </ul>
                <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
                    <span class="fas fa-bars"></span>
                </button>
            </div>
        </nav>
        <!-- #endregion -->
        <div class="container-fluid page-body-wrapper">
            <!-- #region  BARRA LATERAL -->
            <nav class="sidebar sidebar-offcanvas" id="sidebar">
                <ul class="nav">
                    <li class="nav-item nav-profile">
                        <a class="navbar-brand brand-logo" href="#"><img src="~/images/LogoAustral.png" alt="logo" width="250" /></a>
                    </li>
                    <li class="nav-item nav-profile">
                        <div class="nav-link">
                            <div class="profile-name">
                                <p class="name font-weight-light">
                                    <i class="fa fa-user  menu-icon" aria-hidden="true"></i>  TENIS
                                </p>
                            </div>
                        </div>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" data-toggle="collapse" href="#uno" aria-expanded="false" aria-controls="form-elements">
                            <i class="fab fa-wpforms menu-icon"></i>
                            <span class="menu-title">Contratos</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="uno">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Sedes" asp-action="Index">Sedes </a></li>
                            </ul>
                        </div>
                    </li>
                </ul>
            </nav>
            <!-- #endregion -->

            <div class="main-panel">
                @RenderBody()
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                <footer class="footer">
                    <div class="d-sm-flex justify-content-center justify-content-sm-between">
                        <span class="text-muted text-center text-sm-left d-block d-sm-inline-block">Copyright © 2021 - Myper SAC - v1.0.1</span>
                    </div>
                </footer>
                <!-- partial -->
            </div>
        </div>
    </div>

    <environment include="Development">
        <!-- #region PLANTILLA -->
        <script src="~/vendors/js/vendor.bundle.base.js"></script>
        <script src="~/vendors/js/vendor.bundle.addons.js"></script>
        <script src="~/js/off-canvas.js"></script>
        <script src="~/js/hoverable-collapse.js"></script>
        <script src="~/js/misc.js"></script>
        <script src="~/js/settings.js"></script>
        <script src="~/js/todolist.js"></script>
        <script src="~/js/dashboard.js"></script>
        <script src="../../js/owl-carousel.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>

        <script src="~/vendors/toastr/toastr.min.js"></script>
        <script src="~/vendors/form-master/dist/jquery.form.min.js"></script>
        <script src="../../vendors/summernote/dist/summernote-bs4.min.js"></script>
        <script src="../../js/editorDemo.js"></script>
        <script asp-append-version="true" src="~/vendors/js/CustomForm.js"></script>
        <script asp-append-version="true" src="~/vendors/js/ResponseRequest.js"></script>
        <!-- #endregion -->
    </environment>
    <environment exclude="Development">
        <!-- #region PLANTILLA -->
        <script src="~/vendors/js/vendor.bundle.base.js"></script>
        <script src="~/vendors/js/vendor.bundle.addons.js"></script>

        <script src="~/js/off-canvas.js"></script>
        <script src="~/js/hoverable-collapse.js"></script>
        <script src="~/js/misc.js"></script>
        <script src="~/js/settings.js"></script>
        <script src="~/js/todolist.js"></script>
        <script src="~/js/dashboard.js"></script>
        <script src="../../js/owl-carousel.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>

        <script src="~/vendors/toastr/toastr.min.js"></script>
        <script src="~/vendors/form-master/dist/jquery.form.min.js"></script>
        <script src="../../vendors/summernote/dist/summernote-bs4.min.js"></script>
        <script src="../../js/editorDemo.js"></script>
        <script asp-append-version="true" src="~/vendors/js/CustomForm.js"></script>
        <script asp-append-version="true" src="~/vendors/js/ResponseRequest.js"></script>
        <!-- #endregion -->
    </environment>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
